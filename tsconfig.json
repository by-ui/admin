{
    "include": ["@declaration", "src"],
    "exclude": ["node_modules"],
    "compilerOptions": {
        "jsx": "preserve",
        "jsxFactory": "h",
        "module": "esnext",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "baseUrl": "./",
        "paths": {
            "@/*": ["./src/*"],
        },
    }
}
